{
  "presets": [["@babel/preset-env", {
    "targets": [
      "last 2 versions",
      "not < 2%",
      "not dead",
      "not ie 11"
    ],
    "useBuiltIns": "entry"
  }], "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}